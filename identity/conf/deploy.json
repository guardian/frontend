{
    "packages":{
        "frontend-identity":{
            "type":"asg-elb",
            "apps":[ "frontend::identity" ],
            "data":{
                "secondsToWait":900,
                "port":18080,
                "healthcheckGrace":20,
                "bucket":"aws-frontend-artifacts",
                "healthcheck_paths":[
                    "/management/healthcheck"
                ]
            }
        }
    },
    "recipes":{
        "default":{
            "depends" : ["deploy"]
        },
        "deploy":{
            "actionsBeforeApp": ["frontend-identity.deploy"]
        }
    }
}