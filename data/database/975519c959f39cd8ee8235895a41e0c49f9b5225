      "id":"music/2013/oct/06/glastonbury-festival-tickets-sell-record-time",
      "sectionId":"music",
      "sectionName":"Music",
      "webPublicationDate":"2013-10-06T15:26:00Z",
      "webTitle":"Glastonbury festival 2014 tickets sell out in record time",
      "webUrl":"http://www.theguardian.com/music/2013/oct/06/glastonbury-festival-tickets-sell-record-time",
