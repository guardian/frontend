{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./static/src/javascripts/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "esnext",
      "es2015",
      "dom",
      "dom.iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "paths": {
      "admin/*": [
        "./projects/admin/*"
      ],
      "bootstraps/*": [
        "./bootstraps/*"
      ],
      "commercial/*": [
        "./projects/commercial/*"
      ],
      "common/*": [
        "./projects/common/*"
      ],
      "facia/*": [
        "./projects/facia/*"
      ],
      "journalism/*": [
        "./projects/journalism/*"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "lodash/*": [
        "node_modules/lodash-node/compat/*"
      ],
      "membership/*": [
        "./projects/membership/*"
      ],
      "ophan/embed": [
        "node_modules/ophan-tracker-js/build/ophan.embed"
      ],
      "ophan/ng": [
        "node_modules/ophan-tracker-js"
      ],
      "raven": [
        "node_modules/raven-js"
      ],
      "svgs/*": [
        "./static/src/inline-svgs/*"
      ],
      "videojs": [
        "node_modules/video.js"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNEXT"
  },
  "include": [
    "./static/src/javascripts"
  ]
}
