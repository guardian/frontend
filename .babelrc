{
    "presets": [
        "babel-preset-flow"
    ],
    "plugins": [
        "babel-plugin-transform-object-rest-spread",
        "babel-plugin-syntax-dynamic-import"
    ],
    "env": {
        "production": {
            "presets": [
                ["babel-preset-env", {
                    "modules": "amd"
                }]
            ],
            "plugins": [
                ["babel-plugin-transform-runtime", {
                    "polyfill": false
                }],
                "babel-plugin-add-module-exports",
                "babel-plugin-transform-class-properties",
            ],
        },
        "development": {
            "presets": [
                ["babel-preset-env", {
                    "modules": "amd"
                }]
            ],
            "plugins": [
                ["babel-plugin-transform-runtime", {
                    "polyfill": false
                }],
                "babel-plugin-add-module-exports",
                "babel-plugin-transform-class-properties",
            ],
        },
        "test": {
            "presets": [
                ["babel-preset-env", {
                    "targets": {
                        "node": "current"
                    },
                    "useBuiltIns": true
                }]
            ],
            "plugins": [
                ["babel-plugin-transform-runtime", {
                    "polyfill": false
                }],
                "babel-plugin-transform-class-properties",
            ],
        },
        "internal": {
            "presets": [
                ["babel-preset-env", {
                    "targets": {
                        "browsers": [
                            "last 2 Chrome versions",
                            "last 1 Safari version",
                            "last 2 Firefox versions",
                            "last 2 Edge versions",
                        ],
                    }
                }],
            ],
        },
        "karma": {
            "presets": [
                ["babel-preset-env", {
                    "modules": "amd"
                }]
            ],
            "plugins": [
                "babel-plugin-add-module-exports",
                "babel-plugin-transform-class-properties",
            ],
        },
    },
    "ignore": [
        "eslintrc.js",
    ],
}
