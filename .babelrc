{
    "env": {
        "production": {
            "presets": [
                "latest"
            ],
            "plugins": [
                "add-module-exports",
                "transform-es2015-modules-amd"
            ]
        },
        "development": {
            "plugins": [
                "transform-async-to-generator"
            ]
        },
        "admin": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": ["last 2 Chrome versions", "last 1 Safari version", "last 2 Firefox versions", "last 2 Edge versions"]
                    }
                }]
            ]
        }
    }
}
