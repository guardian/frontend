{
    "env": {
        "production": {
            "presets": [
                [
                    "babel-preset-env",
                    {
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                "babel-plugin-syntax-dynamic-import",
                "babel-plugin-transform-runtime"
            ]
        },
        "development": {
            "presets": [
                [
                    "babel-preset-env",
                    {
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                "babel-plugin-syntax-dynamic-import",
                "babel-plugin-transform-runtime"
            ]
        },
        "internal": {
            "presets": [
                [
                    "babel-preset-env",
                    {
                        "targets": {
                            "browsers": [
                                "last 2 Chrome versions",
                                "last 1 Safari version",
                                "last 2 Firefox versions",
                                "last 2 Edge versions"
                            ]
                        }
                    }
                ]
            ]
        },
        "karma": {
            "presets": [
                [
                    "babel-preset-env",
                    {
                        "modules": "amd"
                    }
                ]
            ],
            "plugins": [
                "babel-plugin-dynamic-import-amd",
                "babel-plugin-transform-runtime"
            ]
        }
    },
    "ignore": [
        "eslintrc.js"
    ]
}
